         <div xml:id="bHY6yh-d80273"><!-- p4l3-r13 -->
            <head xml:id="bHY6yh-d80273-Hd1e108">Rubrica 13</head>
            <head xml:id="bHY6yh-d80273-Hd1e111" type="question-title">De laude et commendatione</head>
            <p xml:id="bHY6yh-d80273-d1e114">
              <lb ed="#Ka" n="4"/>De laude et commendatione 
              <lb ed="#Ka"/>Rubrica 13 //
            </p>
            <p xml:id="bHY6yh-d80273-d1e119">
              <lb ed="#Ka"/><ref xml:id="bHY6yh-d80273-Rd1e116">IOb hystoria capitulo 18</ref> // <quote xml:id="bHY6yh-d80273-Qd1e118">lux im<lb ed="#Ka" break="no"/>pii 
              destiguetur nec splen<lb ed="#Ka" break="no"/>debit
              flamma ignis eius</quote> 
            </p>
            <p xml:id="bHY6yh-d80273-d1e128"> 
              OMITTED
            </p>
            <p xml:id="bHY6yh-d80273-d1e135">
              <ref xml:id="bHY6yh-d80273-Rd1e141">Psal<lb ed="#Ka" break="no"/>mista 
              32 pslamus</ref> // <quote xml:id="bHY6yh-d80273-Qd1e137">Rectos decet collau<lb ed="#Ka" break="no"/>datio</quote>
            </p>
            <p xml:id="bHY6yh-d80273-d1e139">
              // <ref xml:id="bHY6yh-d80273-Rd1e151">Salomon in prouerbiis capitulo 
              <lb ed="#Ka"/>27</ref> // <quote xml:id="bHY6yh-d80273-Qd1e143">laudet te alienus non et os 
              <lb ed="#Ka"/>tuum</quote>
            </p>
            <p xml:id="bHY6yh-d80273-d1e149"> 
              <ref xml:id="bHY6yh-d80273-Rd1e161">28 capitulo</ref> <quote xml:id="bHY6yh-d80273-Qd1e151">Vir fidelis multum 
              <lb ed="#Ka"/>laudabitur</quote>
            </p>
            <p xml:id="bHY6yh-d80273-d1e157">
              // <ref xml:id="bHY6yh-d80273-Rd1e173">Cronica de nugis philosophurum 
              <lb ed="#Ka"/>capitulo 6 scilicet de biante</ref> <quote xml:id="bHY6yh-d80273-Qd1e161">Bias vnus 
              <lb ed="#Ka"/>de septem sapiententibus ait in dignum 
              <lb ed="#Ka"/>virum ne laudes ob diuitias</quote> //
            </p>
            <p xml:id="bHY6yh-d80273-d1e175">
              <lb ed="#Ka"/><ref xml:id="bHY6yh-d80273-Rd1e193">Aristotiles ethycorum libro 1 capitulo 7</ref> <quote xml:id="bHY6yh-d80273-Qd1e179">videtur 
              <lb ed="#Ka"/>animo laudabile in equale quid 
              <lb ed="#Ka"/>esse et aliquid qualiter habere laudari 
              <lb ed="#Ka"/>Sunt laudes per cellationem laus 
              <lb ed="#Ka"/>est operum similiter et corporali<lb ed="#Ka" break="no"/>um 
              et animalium</quote>
            </p>
            <p xml:id="bHY6yh-d80273-d1e189">
              <lb ed="#Ka"/><ref xml:id="bHY6yh-d80273-Rd1e209">Idem magnorum moralium <del>et</del>
              <lb ed="#Ka"/>libro 1 capitulo 42</ref> <quote xml:id="bHY6yh-d80273-Qd1e193">liberalis est laudabilis</quote>
            </p>
            <p xml:id="bHY6yh-d80273-d1e199"> 
              <lb ed="#Ka"/>/ <ref xml:id="bHY6yh-d80273-Rd1e220">7 capitulo</ref> <quote xml:id="bHY6yh-d80273-Qd1e201">laudabiles sunt prudentes 
              <lb ed="#Ka"/>laus virtus est</quote>
            </p>
            <p xml:id="bHY6yh-d80273-d1e208"> 
              <ref xml:id="bHY6yh-d80273-Rd1e230">Idem rethoricorum 
              <lb ed="#Ka"/>libro 1 capitulo 16</ref> <quote xml:id="bHY6yh-d80273-Qd1e210">laus est sermo illucidans 
              <lb ed="#Ka"/>magnitudinem virtutis Con<cb ed="#Ka" n="b"/><lb ed="#Ka"/>mendatio 
              operum est</quote>
            </p>
            <p xml:id="bHY6yh-d80273-d1e240">
              <ref xml:id="bHY6yh-d80273-Rd1e242">libro 1 capitulo 9</ref> <quote xml:id="bHY6yh-d80273-Qd1e246">lau<lb ed="#Ka" break="no"/>dare 
              presentem adulationis est</quote> //
            </p>
            <p xml:id="bHY6yh-d80273-d1e220">
              <lb ed="#Ka"/><ref xml:id="bHY6yh-d80273-Rd1e253">Ihesus filius syrach in ecclesiastico 
              <lb ed="#Ka"/>capitulo 15</ref> <quote xml:id="bHY6yh-d80273-Qd1e224">Non est speciosa laus in ore 
              <lb ed="#Ka"/>peccatorum</quote>
            </p>
            <p xml:id="bHY6yh-d80273-d1e230">
              <lb ed="#Ka"/><ref xml:id="bHY6yh-d80273-Rd1e265">Oratius in epistolis libro 1 epistola 19</ref> <quote xml:id="bHY6yh-d80273-Qd1e234">Qua<lb ed="#Ka" break="no"/>lem 
              commendes etiam aspice ne 
              <lb ed="#Ka"/>mox incutiant aliena ubi peccata 
              <lb ed="#Ka"/>pudorem</quote>
            </p>
            <p xml:id="bHY6yh-d80273-d1e242">
              <ref xml:id="bHY6yh-d80273-Rd1e279">Libro 2 epistola 2</ref> <quote xml:id="bHY6yh-d80273-Qd1e259">Plenius equo 
              <lb ed="#Ka"/>laudat venales qui uult excludere 
              <lb ed="#Ka"/>merces</quote>
            </p>
            <p xml:id="bHY6yh-d80273-d1e252">
              <lb ed="#Ka"/><ref xml:id="bHY6yh-d80273-Rd1e292">Tulius rethoricorum artis noue libro 2 capitulo 2</ref> 
              <lb ed="#Ka"/><quote xml:id="bHY6yh-d80273-Qd1e256">Non solum laudis causa rectum 
              <lb ed="#Ka"/>sequi conuenit sed si laus sequitur 
              <lb ed="#Ka"/>duplicatur causa appetendi uolun<lb ed="#Ka" break="no"/>tas</quote>
            </p>
            <p xml:id="bHY6yh-d80273-d1e264">
              <ref xml:id="bHY6yh-d80273-Rd1e306">Idem philippicorum libro 2</ref> <quote xml:id="bHY6yh-d80273-Qd1e268">vt 
              <lb ed="#Ka"/>quondam morbo aliqui exten<lb ed="#Ka" break="no"/>sus 
              stupore suauitatem tibi non 
              <lb ed="#Ka"/>sentiunt sic libidinosi auari fa<lb ed="#Ka" break="no"/>cinorosi 
              vere laudis gestum non 
              <lb ed="#Ka"/>habent</quote>
            </p>
            <p xml:id="bHY6yh-d80273-d1e279">
              // <ref xml:id="bHY6yh-d80273-Rd1e322">Ouidius epistolarum epistola 12</ref> <quote xml:id="bHY6yh-d80273-Qd1e283">vt 
              <lb ed="#Ka"/>culpent alii tibi me laudare necesse 
              <lb ed="#Ka"/>esse est</quote>
            </p>
            <p xml:id="bHY6yh-d80273-d1e289">
              // <ref xml:id="bHY6yh-d80273-Rd1e334">Idem de ponto libro 4 et finali epistola 
              <lb ed="#Ka"/>2</ref> // <quote xml:id="bHY6yh-d80273-Qd1e293">Excitat auditor studium lau<lb ed="#Ka" break="no"/>data
              que virtus Crescit et in mensum 
              <lb ed="#Ka"/>gloria calcar habet</quote>
            </p>
            <p xml:id="bHY6yh-d80273-d1e301">
              <lb ed="#Ka"/><ref xml:id="bHY6yh-d80273-Rd1e348">Paulus apostolus epistola 2 ad corinthe<lb ed="#Ka" break="no"/>os 
              capitulo 10</ref> <quote xml:id="bHY6yh-d80273-Qd1e305">qui gloriatur In domino 
              <lb ed="#Ka"/>glorietur non enim qui se ipsum 
              <pb ed="#Ka" n="179-r"/>
              <cb ed="#Ka" n="a"/>
              <lb ed="#Ka"/>commendat ille probatus est sed quem 
              <lb ed="#Ka"/>deus commendat</quote>
            </p>
            <p xml:id="bHY6yh-d80273-d1e313">
              // <ref xml:id="bHY6yh-d80273-Rd1e362">Seneca in epistolis 
              <lb ed="#Ka"/>ad lucillum libro 7 epistola 4</ref> <quote xml:id="bHY6yh-d80273-Qd1e317">Quid letaris 
              <lb ed="#Ka"/>quod ab hominibus hiis lauditus es
              <lb ed="#Ka"/>quos non potes ipse laudare Qualisque 
              <lb ed="#Ka"/>sit sciens si quemadmodum laudet 
              <lb ed="#Ka"/>aspexerit</quote>
            </p>
            <p xml:id="bHY6yh-d80273-d1e327"> 
              <ref xml:id="bHY6yh-d80273-Rd1e377">libro 18 epistola 2</ref> // <quote xml:id="bHY6yh-d80273-Qd1e329">Magnisicum 
              <lb ed="#Ka"/>est laudari alaudato viro laude 
              <lb ed="#Ka"/>digno merentem laudare iust<lb ed="#Ka" break="no"/>itia 
              est</quote>
            </p>
            <p xml:id="bHY6yh-d80273-d1e339"> 
              <ref xml:id="bHY6yh-d80273-Rd1e392">Idem de transquillitate ani<lb ed="#Ka" break="no"/>mi</ref> 
              <quote xml:id="bHY6yh-d80273-Qd1e341">Quis non inter laudantium 
              <lb ed="#Ka"/>blandientiumque positus gregres 
              <lb ed="#Ka"/>plurimum dende ipse sibi assentatus 
              <lb ed="#Ka"/>est</quote>
            </p>
            <p xml:id="bHY6yh-d80273-d1e350">
              <lb ed="#Ka"/><ref xml:id="bHY6yh-d80273-Rd1e406">Idem de remedio fortuitorum capitulo 8</ref> <quote xml:id="bHY6yh-d80273-Qd1e354">dis<lb ed="#Ka" break="no"/>plicere 
              magis laudari est</quote>
            </p>
            <p xml:id="bHY6yh-d80273-d1e360"> 
              <ref xml:id="bHY6yh-d80273-Rd1e416">Idem de 
              <lb ed="#Ka"/>moribus fortuitorum</ref> <quote xml:id="bHY6yh-d80273-Qd1e362">Neminem 
              <lb ed="#Ka"/>cito accusaueris</quote>
            </p>
            <p xml:id="bHY6yh-d80273-d1e374">
              <ref xml:id="bHY6yh-d80273-Rd1e434">Plinius de n<lb ed="#Ka" break="no"/>aturali 
              d hystoria in prohemino</ref> <quote xml:id="bHY6yh-d80273-Qd1e378">Be<lb ed="#Ka" break="no"/>ningnum 
              est et plenum ingenii pud<lb ed="#Ka" break="no"/>oris
              profiteri per quos proficeris</quote>
            </p>
            <p xml:id="bHY6yh-d80273-d1e386">
              <lb ed="#Ka"/><ref xml:id="bHY6yh-d80273-Rd1e451">Ualerius maximus libro 4 titulo de pau<lb ed="#Ka" break="no"/>pertate
              capitulo 4</ref> <quote xml:id="bHY6yh-d80273-Qd1e390">Non est necessaria soli<lb ed="#Ka" break="no"/>de 
              laudi cupidis anxia diuitiarum 
              <lb ed="#Ka"/>comparitio</quote>
            </p>
            <p xml:id="bHY6yh-d80273-d1e398">
              // <ref xml:id="bHY6yh-d80273-Rd1e467">Marcialis cocus libro vn<lb ed="#Ka" break="no"/>dique 
              suscepto capitulo 2</ref> <quote xml:id="bHY6yh-d80273-Qd1e402">vituperat subi<lb ed="#Ka" break="no"/>to 
              qui laudat alebi / Nullum credo 
              <lb ed="#Ka"/>decus laudari A turpibus herme 
              <lb ed="#Ka"/>sed nec laudari ob turpia credo decus 
              <lb ed="#Ka"/>premia uel laudes ob nuimus sceleri</quote>
            </p>
            <p xml:id="bHY6yh-d80273-d1e414">
              <cb ed="#Ka" n="b"/>
              <lb ed="#Ka"/><ref xml:id="bHY6yh-d80273-Rd1e485">Auctor liber de formula honeste
              <lb ed="#Ka"/>vite capitulo 2</ref> <quote xml:id="bHY6yh-d80273-Qd1e418">lauda parce uitupera parce 
              <lb ed="#Ka"/>Nam similiter reprehensibilis nimi<lb ed="#Ka" break="no"/>a 
              laudatio quam in mediata vituper<lb ed="#Ka" break="no"/>atio 
              Illa siquidem adulatione ista 
              <lb ed="#Ka"/>malignitate suspecta est non quam 
              <lb ed="#Ka"/>multis sed qualitus placeas cogita</quote>
            </p>
            <p xml:id="bHY6yh-d80273-d1e433"> 
              <lb ed="#Ka"/><ref xml:id="bHY6yh-d80273-Rd1e503">capitulo 4</ref> <quote xml:id="bHY6yh-d80273-Qd1e435">Sit tam triste tibi laudari a 
              <lb ed="#Ka"/>turpibus ac si lauderis ob turpia 
              <lb ed="#Ka"/>Nec extollas quequam nec deicias</quote>
            </p>
            <p xml:id="bHY6yh-d80273-d1e443">
              <lb ed="#Ka"/><ref xml:id="bHY6yh-d80273-Rd1e517">Cato capitulo 2</ref> <quote xml:id="bHY6yh-d80273-Qd1e447">Cum te aliquis lau<lb ed="#Ka" break="no"/>det 
              iudex tuus esse memento Plus 
              <lb ed="#Ka"/>aliis dete quam tu tibi credere 
              <lb ed="#Ka"/>noli</quote>
            </p>
            <p xml:id="bHY6yh-d80273-d1e455">
              <ref xml:id="bHY6yh-d80273-Rd1e531">capitulo 3</ref> <quote xml:id="bHY6yh-d80273-Qd1e459">Nec te collaudes nec te 
              <lb ed="#Ka"/>culpaueris ipsum hoc faciunt stulti 
              <lb ed="#Ka"/>quos gloria vexat in anis</quote>
            </p>
            <p xml:id="bHY6yh-d80273-d1e467">
              <ref xml:id="bHY6yh-d80273-Rd1e545"></ref> <quote xml:id="bHY6yh-d80273-Qd1e471">lau<lb ed="#Ka" break="no"/>deris 
              quicumque palam quecumque proba<lb ed="#Ka" break="no"/>rus
              // hoc uide ne rursus leui<lb ed="#Ka" break="no"/>tatis 
              crimine dampnes Parce lau<lb ed="#Ka" break="no"/>dato 
              nam quem tu sepe probaris vna 
              <lb ed="#Ka"/>dies qualis fuerit mirabit amicum</quote>
            </p>
            <p xml:id="bHY6yh-d80273-d1e483">
              <lb ed="#Ka"/><ref xml:id="bHY6yh-d80273-Rd1e564">Boetius de disciplina scolarium 
              <lb ed="#Ka"/>libro 3</ref> <quote xml:id="bHY6yh-d80273-Qd1e487">Rerum effectus exhibitione op<lb ed="#Ka" break="no"/>eris 
              declaratur</quote>
            </p>
            <p xml:id="bHY6yh-d80273-d1e493">
              <lb ed="#Ka"/><ref xml:id="bHY6yh-d80273-Rd1e576">Cassiodorus epistolarum libro primo epistola 2</ref> 
              <lb ed="#Ka"/><quote xml:id="bHY6yh-d80273-Qd1e497">Quid magis queritur quam ibi in uenis<lb ed="#Ka" break="no"/>se 
              laudum testimonia vbi gratifi<lb ed="#Ka" break="no"/>catio 
              non potest esse suspecta</quote>
            </p>
            <p xml:id="bHY6yh-d80273-d1e508"> 
              <ref xml:id="bHY6yh-d80273-Rd1e590">epistola 3</ref> 
              <lb ed="#Ka"/><quote xml:id="bHY6yh-d80273-Qd1e510">Crescere virtutes solent ad preconii 
              <pb ed="#Ka" n="179-v"/>
              <cb ed="Ka" n="a"/>
              <lb ed="#Ka"/>uestigium</quote>
            </p>
            <p xml:id="bHY6yh-d80273-d1e516"> 
              <ref xml:id="bHY6yh-d80273-Rd1e600">libro 4 epistola 3</ref> <quote xml:id="bHY6yh-d80273-Qd1e518">Cum successor 
              <lb ed="#Ka"/>amplius laudari nititur prece<lb ed="#Ka" break="no"/>dentis 
              fama letatur</quote>
            </p>
            <p xml:id="bHY6yh-d80273-d1e524">
              <ref xml:id="bHY6yh-d80273-Rd1e612">libro 4 capitulo 30</ref> <quote xml:id="bHY6yh-d80273-Qd1e528">Per<lb ed="#Ka" break="no"/>fectum 
              opus suum laudat auctorem</quote>
            </p>
            <p xml:id="bHY6yh-d80273-d1e534"> 
              <lb ed="#Ka"/><!--unknown symbol here--><ref xml:id="bHY6yh-d80273-Rd1e622">31 epistola</ref> <quote xml:id="bHY6yh-d80273-Qd1e536">Prudens frequentiam non 
              <lb ed="#Ka"/>respuat hominum in qua se nouit 
              <lb ed="#Ka"/>esse laudandum alioquin virtutibus 
              <lb ed="#Ka"/>fama tollitur</quote>
            </p>
            <p xml:id="bHY6yh-d80273-d1e546"> 
              <ref xml:id="bHY6yh-d80273-Rd1e637">libro 9 epistola 22</ref> <quote xml:id="bHY6yh-d80273-Qd1e548">Magnus 
              <lb ed="#Ka"/>verecundie stimulus est laus pa<lb ed="#Ka" break="no"/>rentum 
              dum illis non patimur 
              <lb ed="#Ka"/>esse in pares quos gaudemus 
              <lb ed="#Ka"/>auctores</quote>
            </p>
            <p xml:id="bHY6yh-d80273-d1e565"> 
              <ref xml:id="bHY6yh-d80273-Rd1e657">25 capitulo et finali</ref> <quote xml:id="bHY6yh-d80273-Qd1e567">Gloriosis
              <lb ed="#Ka"/>dominis gratiora sunt preconia quam tri<lb ed="#Ka" break="no"/>buta</quote>
            </p>
            <p xml:id="bHY6yh-d80273-d1e574">
              <ref xml:id="bHY6yh-d80273-Rd1e669">libro 10 epistola 23</ref> <quote xml:id="bHY6yh-d80273-Qd1e578">Iuste potiora con<lb ed="#Ka" break="no"/>sequitur 
              qui de commissa sibi nego<lb ed="#Ka" break="no"/>cii 
              perfectione laudatur</quote>
            </p>
            <p xml:id="bHY6yh-d80273-d1e584">
              <lb ed="#Ka"/><ref xml:id="bHY6yh-d80273-Rd1e681">Auctor liber qui dicitur facetus 
              <lb ed="#Ka"/>capitulo 2</ref> <quote xml:id="bHY6yh-d80273-Qd1e588">alterius laudes modera<lb ed="#Ka" break="no"/>re 
              dicere laudo Sed proprias nemo 
              <lb ed="#Ka"/>sibi sapit ipse refert</quote>
            </p>
            <p xml:id="bHY6yh-d80273-d1e596">
              // <ref xml:id="bHY6yh-d80273-Rd1e695">Andreas 
              <lb ed="#Ka"/>ad gualterium de amore libro 1</ref> <quote xml:id="bHY6yh-d80273-Qd1e600">Ca<lb ed="#Ka" break="no"/>lidi 
              videntur blandimenti opti<lb ed="#Ka" break="no"/>nere
              commentum laudes aspectum laudate 
              <lb ed="#Ka"/>prolate</quote>
            </p>
            <p xml:id="bHY6yh-d80273-d1e608">
              <lb ed="#Ka"/><ref xml:id="bHY6yh-d80273-Rd1e709">Matheus vindocinensis de hyst<lb ed="#Ka" break="no"/>oria 
              thobie capitulo 30</ref> <quote xml:id="bHY6yh-d80273-Qd1e612">Se laudi con<lb ed="#Ka" break="no"/>formet 
              opus nisi facti marites
              <lb ed="#Ka"/>verbis verba nocent friuola fruc<lb ed="#Ka" break="no"/>tus 
              abest</quote>
            </p>
            <p xml:id="bHY6yh-d80273-d1e620">
              // <ref xml:id="bHY6yh-d80273-Rd1e724">Prouerbium uulgare</ref> 
              <cb ed="#Ka" n="b"/>
              <lb ed="#Ka"/><quote xml:id="bHY6yh-d80273-Qd1e624">la onra lauda el maistro chi la<lb ed="#Ka" break="no"/>do 
              el mato fa salire</quote>
            </p>
         </div>
      </body>
   </text>
